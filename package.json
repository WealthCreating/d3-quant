{
    "version": "0.1.7",
    "name": "d3-quant",
    "title": "d3-quant",
    "description": "D3 library for quantitative analytics",
    "homepage": "https://github.com/quantmind/d3-quant",
    "repository": {
        "type": "git",
        "url": "https://github.com/quantmind/d3-quant.git"
    },
    "keywords": [
        "d3",
        "math"
    ],
    "license": "BSD-3-Clause",
    "author": {
        "name": "quantmind.com",
        "email": "message@quantmind.com"
    },
    "main": "build/d3-quant.js",
    "jsnext:main": "index",
    "dependencies": {
        "d3-array": "1.0",
        "d3-collection": "1.0",
        "d3-random": "1.0"
    },
    "devDependencies": {
        "babelify": "7.3",
        "babel-preset-es2015-rollup": "1.2",
        "babel-tape-runner": "2.0",
        "browserify": "13.1",
        "covert": "1.1",
        "crossfilter": "1.3",
        "eslint": "3.3",
        "faucet": "0.0",
        "rollup": "0.34",
        "rollup-plugin-babel": "2.6",
        "rollup-plugin-json": "2.0",
        "tape": "4.6",
        "uglify-js": "2.7"
    },
    "scripts": {
        "pretest": "rollup -c",
        "test": "eslint index.js src test && babel-tape-runner `find test -name '*-test.js'` | faucet",
        "prepublish": "npm run test && uglifyjs build/d3-quant.js -c -m -o build/d3-quant.min.js && rm -f build/d3-quant.zip && zip -j build/d3-quant.zip -- LICENSE README.md build/d3-quant.js build/d3-quant.min.js",
        "postpublish": "VERSION=`node -e 'console.log(require(\"./package.json\").version)'`; git push && git tag -am \"Release $VERSION.\" v${VERSION} && git push --tags"
    },
    "babel": {
        "presets": ["es2015"]
    }
}
